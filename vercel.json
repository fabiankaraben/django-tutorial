{
    "version": 2,
    "builds": [
        {
            "src": "django_tutorial/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/staticfiles/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "django_tutorial/wsgi.py"
        }
    ],
    "outputDirectory": "staticfiles"
}