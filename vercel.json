{
    "version": 2,
    "builds": [
        {
            "src": "django_tutorial/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "django_tutorial/wsgi.py"
        }
    ],
    "outputDirectory": "public"
}